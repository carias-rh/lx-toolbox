curl -s  'https://rol.redhat.com/rol/api/courses/' \
  -H 'x-csrftoken: mYV7ANBSuKiItqLicTOM0vlO6SRUk3Lb' \
   -H 'cookie: csrftoken=mYV7ANBSuKiItqLicTOM0vlO6SRUk3Lb; sessionid=rmm5t64dra7z9tojwfpjpjcxe95ze583' \
  --compressed | jq -r '.[].path' |uniq |egrep -iv  "^$|_|custom|vc|premium|plus|subscription|sku|gss|boot-camp|enablement|exam|support" | egrep "[a-z][a-z][0-9]*[ea]*-[0-9]\.[0-9]"  > ./courses-list.txt
